2) Fiz o codigo na limguagem C
#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <locale.h>
#include<stdbool.h>
main() {

    setlocale(LC_ALL, "Portuguese");

    int a, b, auxiliar, i, n;
	bool esta = false;
    a = 0;
    b = 1;

    printf("Digite um número: ");
    scanf("%d", &n);
    printf("\nOs %d primeiros numeros da Série de Fibonacci:\n\n", n);
    printf("%d\t", b);

    for(i = 0; i < n; i++) {

        auxiliar = a + b;
        a = b;
        b = auxiliar;
        if (auxiliar == n)
        	esta = true;

        printf("%d\t", auxiliar);
    }
    printf("\n");
    if(esta == true)
    	printf("O numero %d está na sequencia de Fibonacci", n);
    else
    	printf("O numero %d não está na sequencia de Fibonacci", n);
}

